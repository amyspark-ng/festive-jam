{
  "typescript": {
    "lineWidth": 120,
    "trailingCommas": "onlyMultiLine",
    "indentWidth": 4,
    "semiColons": "always"
  },
  "json": {
    "indentWidth": 2
  },
  "markdown": {
  },
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.93.3.wasm",
    "https://plugins.dprint.dev/json-0.19.4.wasm",
    "https://plugins.dprint.dev/markdown-0.17.8.wasm"
  ]
}
